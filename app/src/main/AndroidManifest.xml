<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.silva.murillo.fitnessapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_icon_app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_icon_app"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitnessApp">

        <activity
            android:name=".ui.view.onboarding.ListOnboardingActivity"
            android:exported="false">
        </activity>

        <activity
            android:name=".ui.view.onboarding.HomeOnboarding"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".HomeActivity"
            android:exported="true" />

    </application>

</manifest>